                    "dynamodb:Scan",
                    "dynamodb:GetItem",
                    "dynamodb:PutItem",
                    "dynamodb:UpdateItem",
                    "dynamodb:DeleteItem",
                    "dynamodb:TransactWriteItems",
                    "dynamodb:CreateTable",
                    "dynamodb:*"
