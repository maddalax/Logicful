{
	"name": "sapper-typescript-graphql-template",
	"description": "A template that includes Svelte with Sapper, TypeScript preprocessing, and a GraphQL server through TypeGraphQL",
	"keywords": [
		"sapper",
		"typegraphql",
		"typescript",
		"eslint",
		"svelte",
		"apollo-server",
		"graphql"
	],
	"homepage": "https://github.com/babichjacob/sapper-typescript-graphql-template",
	"repository": {
		"type": "git",
		"url": "https://github.com/babichjacob/sapper-typescript-graphql-template.git"
	},
	"license": "MIT",
	"version": "2020.08.20",
	"scripts": {
		"tsc": "tsc",
		"validate": "cross-env NODE_PATH=src svelte-check --ignore src/node_modules/@sapper",
		"validate:dev": "cross-env NODE_PATH=src svelte-check --ignore src/node_modules/@sapper --watch",
		"sapper:dev": "cross-env NODE_PATH=src PORT=5000 sapper dev",
		"sapper:build": "cross-env NODE_ENV=production NODE_PATH=src sapper build",
		"sapper:export": "cross-env NODE_ENV=production NODE_PATH=src sapper export --legacy",
		"dev": "cross-env NODE_PATH=src PORT=5000 run-p validate:dev sapper:dev",
		"prod": "cross-env NODE_PATH=src run-s validate sapper:build",
		"start": "node __sapper__/build",
		"export": "run-s validate sapper:export",
		"pretty": "prettier --write --svelte-sort-order scripts-markup-styles ./src/**/*{.svelte,.ts}",
		"http-server": "cd src/data && http-server --cors"
	},
	"dependencies": {
		"@fortawesome/fontawesome-free": "^5.14.0",
		"@types/clusterize.js": "^0.17.1",
		"@types/quill": "^2.0.3",
		"@types/uuid": "^8.3.0",
		"bootstrap": "5.0.0-alpha1",
		"bowser": "^2.10.0",
		"compression": "^1.7.4",
		"countup.js": "^2.0.5",
		"dragula": "^3.7.2",
		"entities": "^2.0.3",
		"express": "^4.17.1",
		"flatpickr": "^4.6.6",
		"fuse.js": "6.4.1",
		"lodash.get": "4.4.2",
		"lodash.has": "4.5.2",
		"lodash.set": "4.3.2",
		"node-fetch": "^2.6.0",
		"nouislider": "11.0.3",
		"onscreen": "1.3.4",
		"polka": "next",
		"popper.js": "1.16.0",
		"quill": "^1.3.7",
		"rollup-plugin-copy": "^3.3.0",
		"sirv": "^1.0.6",
		"sirv-cli": "^1.0.0",
		"smooth-scroll": "^16.1.0",
		"svelte-select": "^3.11.1",
		"traverse": "^0.6.6",
		"uuid": "^8.3.0",
		"vivus": "^0.4.5",
		"waypoints": "4.0.1"
	},
	"devDependencies": {
		"@babel/core": "^7.11.1",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/plugin-transform-runtime": "^7.11.0",
		"@babel/preset-env": "^7.11.0",
		"@babel/runtime": "^7.11.2",
		"@rollup/plugin-babel": "^5.2.0",
		"@rollup/plugin-commonjs": "^15.0.0",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^9.0.0",
		"@rollup/plugin-replace": "^2.3.3",
		"@rollup/plugin-typescript": "^5.0.2",
		"@types/compression": "^1.7.0",
		"@types/express": "^4.17.7",
		"@types/lodash.get": "^4.4.6",
		"@types/lodash.has": "4.5.6",
		"@types/lodash.set": "^4.3.6",
		"@types/node-fetch": "^2.5.7",
		"@typescript-eslint/eslint-plugin": "^3.9.1",
		"@typescript-eslint/parser": "^3.9.1",
		"apollo-server-express": "^2.16.1",
		"bufferutil": "^4.0.1",
		"class-validator": "^0.12.2",
		"cross-env": "^7.0.2",
		"eslint": "^7.7.0",
		"eslint-config-airbnb-base": "^14.2.0",
		"eslint-plugin-import": "^2.22.0",
		"eslint-plugin-svelte3": "^2.7.3",
		"graphql": "^15.3.0",
		"npm-run-all": "^4.1.5",
		"reflect-metadata": "^0.1.13",
		"rollup": "^2.26.4",
		"rollup-plugin-css-only": "^2.1.0",
		"rollup-plugin-svelte": "^5.2.3",
		"rollup-plugin-svg": "^2.0.0",
		"rollup-plugin-terser": "^7.0.0",
		"sapper": "0.28.2",
		"svelte": "3.24.1",
		"svelte-check": "^1.0.20",
		"svelte-preprocess": "^4.1.1",
		"tslib": "^2.0.1",
		"type-graphql": "^1.0.0",
		"typescript": "^4.0.2",
		"utf-8-validate": "^5.0.2"
	}
}
